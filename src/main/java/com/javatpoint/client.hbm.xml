          <property name="selectedGender" column="Gender"></property>
          <property name="selectedLocation" column="Location"></property>
          <property name="selectedState" column="State"></property>
          <property name="phone" column="Phone"></property>
          <property name="mname" column="MiddleName"></property>
          <property name="addressline2" column="AddressLine2"></property>
          <property name="zipcode1" column="Zipcode1"></property>
          <property name="zipcode2" column="Zipcode2"></property>
          <property name="email" column="Email"></property>
    
